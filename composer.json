{
    "name": "uzulla/phpstorm-inspect-code-cli-runner",
    "description": "CLI tool that runs PhpStorm's code inspections and outputs structured results for easier refactoring and find bugs.",
    "keywords": ["PhpStorm", "inspection", "code quality", "refactoring"],
    "license": "MIT",
    "authors": [
        {
            "name": "Junichi Ishida",
            "email": "zishida@gmail.com",
            "homepage": "https://cfe.jp/"
        },
        {
            "name": "ShopSys",
            "homepage": "https://www.shopsys.com/",
            "role": "Original author"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpStormInspect\\": "src/PhpStormInspect"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpStormInspect\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3",
        "symfony/console": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/process": "^6.0",
        "symfony/dotenv": "^6.0",
        "arvenil/ninja-mutex": "^0.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "bin": [
        "bin/phpstorm-inspect"
    ]
}
